
.item{
		width: 702rpx;
		min-height: 356rpx;
		background-color: #fff;
		box-shadow: 0 4px 15px 0 rgba(230, 228, 228, 0.52);
		display: flex;
		flex-direction: column;
		margin: 22rpx 0;
		border-radius: 10px;
		padding: 20rpx;
		box-sizing: border-box;
}
.item text:nth-child(1){
		color: #333333;
		font-size: 1.1rem;
		margin: 10rpx
}
.item text:nth-child(2){
		color: #98A1BB;
		font-size: 1rem;
		margin-left: 40rpx;
}
.data{
		padding: 20rpx 50rpx 0 40rpx;
}
